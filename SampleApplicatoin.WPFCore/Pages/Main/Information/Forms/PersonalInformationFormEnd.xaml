<Page x:Class="SampleApplicatoin.WPFCore.Pages.Main.Information.Forms.PersonalInformationFormEnd"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SampleApplicatoin.WPFCore.Pages.Main.Information.Forms"
      mc:Ignorable="d" 
        d:DesignHeight="450" d:DesignWidth="800"
  Title="PersonalInformationFormStart">
    <Grid>
        <Border Background="#fff"
            Padding="35 30">
            <StackPanel HorizontalAlignment="Left"
            Orientation="Vertical">
                <TextBlock Text="Signup Request"
               Style="{StaticResource InformationPageFormTitle}"/>
                <StackPanel Orientation="Vertical">
                    <TextBlock Text="Данные отправлены на проверку! Ожидайте подтверждения!"
                           Style="{StaticResource InformationPageFormSubtitle}"
                           Margin="0 30 0 0"/>

                </StackPanel>
            </StackPanel>
        </Border>
    </Grid>
</Page>
